{
  "name": "augmented-melbourne",
  "version": "1.0.0",
  "description": "# Requirements * npm * gulp",
  "main": "index.html",
  "scripts": {
    "copy": "node build.js",
    "clean": "rm -r bower_components && rm -r node_modules",
    "bower": "bower install",
    "browser-sync": "browser-sync start --server --files 'src/*.html' --reload-delay=300 --reload-debounce=500",
    "browser-sync-nonlocal": "browser-sync start --server --https --files 'src/*.html' --reload-delay=300 --reload-debounce=500",
    "deploy": "bower install",
    "dev": "npm run copy && npm run browser-sync",
    "android-dev": "npm run copy && npm run browser-sync-nonlocal"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/H-Plus-Time/augmented-melbourne.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/H-Plus-Time/augmented-melbourne/issues"
  },
  "homepage": "https://github.com/H-Plus-Time/augmented-melbourne#readme",
  "dependencies": {
    "browser-sync": "*",
    "copy": "^0.3.0",
    "fs-promise": "^0.5.0",
    "geopouch": "^2.0.3",
    "jimp": "^0.2.27",
    "qrcode-reader": "^0.2.1",
    "three": "*",
    "worker-pouch": "^1.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-es2015-script": "^1.1.0",
    "del": "^2.2.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.12",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.1",
    "gulp-uglify": "^2.0.0",
    "merge-stream": "^1.0.0",
    "plylog": "^0.4.0",
    "polymer-build": "^0.4.0",
    "bower": "^1.8.0",
    "gulp-imagemin": "^3.1.1"
  }
}
