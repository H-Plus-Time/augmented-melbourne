  "dependencies": {
    "browser-sync": "*",
    "copy": "^0.3.0",
    "fs-promise": "^0.5.0",
    "geopouch": "^2.0.3",
    "jimp": "^0.2.27",
    "qrcode-reader": "^0.2.1",
    "three": "*"
  }
